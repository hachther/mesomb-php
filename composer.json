{
    "version": "3.1.0",
    "name": "hachther/mesomb-php",
    "description": "MeSomb PHP Library",
    "keywords": ["mesomb", "payment processing", "api", "mobile money", "orange money"],
    "type": "library",
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MeSomb\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/MeSomb/Operation/PaymentOperationTest.php"
        ]
    },
    "authors": [
        {
            "name": "Hachther LLC",
            "email": "contact@hachther.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpdocumentor/shim": "^3.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hachther/mesomb-php.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpdocumentor/shim": true
        }
    }
}
